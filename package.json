{
  "name": "nodejs-typescript-modern-starter",
  "version": "1.0.0",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "bin": {
    "nodejs-typescript-modern-starter": "dist/src/index.js",
    "ntms": "dist/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xddq/schema2typebox.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "modern",
    "starter",
    "nodejs typescript modern starter",
    "debugging",
    "ts",
    "esbuild",
    "prettier",
    "typescript5",
    "CI/CD",
    "minimal"
  ],
  "author": "Pierre Dahmani <hi@pierre-dev.com>",
  "license": "MIT",
  "scripts": {
    "bundle": "rimraf dist && esbuild --outdir=dist --sourcemap --bundle --platform=node --target=node20.10.0 ./src/index.ts",
    "compile": "tsc",
    "compile:watch": "tsc -w",
    "dev": "nodemon --watch src --watch test --ext ts,json --exec 'yarn bundle && yarn start'",
    "debug": "yarn bundle && node --enable-source-maps --inspect-brk ./dist/index.js",
    "debug:test": "node --inspect-brk ./node_modules/.bin/jest --runInBand .",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "start": "ng serve",
    "test": "ng test",
    "ng": "ng",
    "build": "ng build",
    "watch": "ng build --watch"
  },
  "dependencies": {
    "@angular/animations": "^17.2.0",
    "@angular/common": "^17.2.0",
    "@angular/compiler": "^17.2.0",
    "@angular/core": "^17.2.0",
    "@angular/forms": "^17.2.0",
    "@angular/platform-browser": "^17.2.0",
    "@angular/platform-browser-dynamic": "^17.2.0",
    "@angular/router": "^17.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.2",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.0",
    "@angular/cli": "^17.2.0",
    "@angular/compiler-cli": "^17.2.0",
    "@babel/core": "7.27.1",
    "@babel/preset-env": "7.27.2",
    "@babel/preset-typescript": "7.27.1",
    "@jest/globals": "29.7.0",
    "@tsconfig/node20": "20.1.5",
    "@types/jasmine": "~5.1.4",
    "@types/jest": "29.5.14",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "esbuild": "0.21.3",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "jasmine-core": "~5.1.2",
    "jest": "29.7.0",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nodemon": "3.1.10",
    "prettier": "3.5.3",
    "rimraf": "5.0.10",
    "typescript": "~5.2.2"
  },
  "packageManager": "yarn@4.2.2"
}
